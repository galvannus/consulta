<%= form_with model: @permission do |form| %>
    <% @permission.errors.full_messages.each do |error| %>
        <div>
            <%= error %>
        </div>
    <% end %>
    <div>
        <%= form.label :nombre %>
        <%= form.text_field :nombre %>
    </div>

    <div>
        <%= form.label :descripcion %>
        <%= form.text_field :descripcion %>
    </div>

    <%= form.submit %>
    
<% end %>